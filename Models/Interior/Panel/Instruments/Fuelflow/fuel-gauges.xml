<?xml version="1.0"?>

  <PropertyList> 
    <path>A-10-fuel-gauge.ac</path>

<!-- fuel L -->
    <animation>
	<type>rotate</type>
	<object-name>needle-l</object-name>
	<property>sim/model/F-15E/consumables/fuel/fuel-dsp-left</property>
	<interpolation> 
		<entry><ind>0</ind><dep>189</dep></entry>
		<entry><ind>3000</ind><dep>270</dep></entry>
		<entry><ind>6000</ind><dep>351</dep></entry>
	</interpolation>
	<center>
		<x-m>0.0023</x-m>
		<y-m>0.0000</y-m>
		<z-m>0.0024</z-m>
	</center>
	<axis> 
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
    </animation>

<!-- fuel R -->
    <animation>
	<type>rotate</type>
	<object-name>needle-r</object-name>
	<property>sim/model/F-15E/consumables/fuel/fuel-dsp-right</property>
	<interpolation> 
		<entry><ind>0</ind><dep>171</dep></entry>
		<entry><ind>3000</ind><dep>90</dep></entry>
		<entry><ind>6000</ind><dep>9</dep></entry>
	</interpolation>
	<center>
		<x-m>0.0023</x-m>
		<y-m>0.0000</y-m>
		<z-m>0.0024</z-m>
	</center>
	<axis> 
		<x>-1</x>
		<y>0</y>
		<z>0</z>
	</axis>
    </animation>

    <!-- fuel selector knob -->
    <animation>
	    <object-name>knob</object-name>
	    <type>rotate</type>
	    <property>sim/model/F-15E/controls/fuel/fuel-dsp-sel</property>
	    <factor>45</factor>
	    <center>
		    <x-m>0.0122</x-m>
		    <y-m>-0.013</y-m>
		    <z-m>-0.08</z-m>
	    </center>
	    <axis>
		    <x>-1</x>
		    <y>0</y>
		    <z>0</z>
	    </axis>
    </animation>
    
    <!-- fuel test indicator -->
    <animation>
	    <object-name>test-button</object-name>
	    <type>translate</type>
	    <property>sim/model/F-15E/controls/fuel/fuel-test-ind</property>
	    <factor>-0.005</factor>
	    <axis>
		    <x>1</x>
		    <y>0</y>
		    <z>0</z>
	    </axis>
	</animation>


