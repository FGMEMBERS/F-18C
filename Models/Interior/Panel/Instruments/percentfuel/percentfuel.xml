<?xml version="1.0"?>
<PropertyList>

 <path>percentfuel.ac</path>

   <animation>
    <type>rotate</type>
    <object-name>needle1</object-name>
    <property>consumables/fuel/total-fuel-norm</property>
    <factor>1</factor>
    <center>
    <x>0</x>
    <y>0</y>
    <z>0</z>
    </center>
 <interpolation>
  <entry><ind>0</ind><dep>0</dep></entry>
  <entry><ind>1.0</ind><dep>270</dep></entry>
 </interpolation>
    <axis>
      <x>-1</x>
      <y> 0</y>
      <z> 0</z>
    </axis>
  </animation>

